{
  "/social/login/google-oauth2/": {
    "target": "http://localhost:8000",
    "secure": false
  },

  "/api/v1/*": {
    "target": "http://localhost:8000",
    "secure": false
  },
  "/status/api/v1/*": {
    "target": "http://localhost:8000",
    "secure": false
  },
  "http://localhost:8000/api/v1/*": {
    "secure": false,
    "changeOrigin": true
  },
  "http://localhost:8000/status/api/v1/*": {
    "secure": false,
    "changeOrigin": true
  }
}
