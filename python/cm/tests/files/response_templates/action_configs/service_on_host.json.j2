{
  "adcm": {
    "config": {
      "global": {
        "adcm_url": null,
        "verification_public_key": "{{ stackdir }}/data/file/adcm.{{ adcm_id }}.global.verification_public_key",
        "accept_only_verified_bundles": false
      },
      "statistics_collection": {
        "url": "adcm-usage-ss.arenadata.io"
      },
      "google_oauth": {
        "client_id": null,
        "secret": null
      },
      "yandex_oauth": {
        "client_id": null,
        "secret": null
      },
      "audit_data_retention": {
        "log_rotation_on_fs": 365,
        "log_rotation_in_db": 365,
        "config_rotation_in_db": 0,
        "retention_period": 1825,
        "data_archiving": false
      },
      "ldap_integration": null,
      "auth_policy": {
        "min_password_length": 12,
        "max_password_length": 20,
        "login_attempt_limit": 5,
        "block_time": 5
      }
    }
  },
  "context": {
    "host_id": {{ host_1.id }},
    "service_id": {{ service.id }},
    "cluster_id": {{ cluster.id }},
    "type": "service"
  },
  "env": {
    "run_dir": "{{ datadir }}/run",
    "log_dir": "{{ datadir }}/log",
    "tmp_dir": "{{ datadir }}/run/{{ job_id }}/tmp",
    "stack_dir": "{{ stackdir }}/data/bundle/{{ cluster_bundle.hash }}",
    "status_api_token": "{{ token }}"
  },
  "job": {
    "id": {{ job_id }},
    "action": "with_config_on_host",
    "job_name": "with_config_on_host",
    "command": "with_config_on_host",
    "script": "playbook.yaml",
    "verbose": true,
    "playbook": "{{ stackdir }}/data/bundle/{{ cluster_bundle.hash }}/playbook.yaml",
    "cluster_id": {{ cluster.id }},
    "hostgroup": "all_params",
    "service_id": {{ service.id }},
    "service_type_id": {{ service_type_id }},
    "params": {"rules": []},
    "config": {
      "boolean": true,
      "secrettext": "awe\nsopme\n\ttext\n",
      "list": [
        "1",
        "5",
        "baset"
      ],
      "variant_inline": "f",
      "plain_group": {
        "file": "{{ filedir }}/task.{{ task_id }}.plain_group.file",
        "map": {
          "k": "v",
          "key": "val"
        },
        "simple": null
      },
      "integer": null,
      "float": null,
      "string": null,
      "password": null,
      "map": {},
      "secretmap": null,
      "json": null,
      "file": null,
      "secretfile": null,
      "variant_builtin": null,
      "activatable_group": null,
      "source_list": ["ok", "fail"]
    }
  }
}